echo "####### RUNNING THE 'RUN' SCRIPT FROM THE BUILDER IMAGE#######"
exec /usr/local/s2i/run
echo "####### FINISHED RUNNING THE 'RUN' SCRIPT FROM THE BUILDER IMAGE#######"
